{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "max-len": ["error", 120, 4, {
            "ignoreUrls": true,
            "ignoreComments": false,
            "ignoreRegExpLiterals": true,
            "ignoreStrings": true,
            "ignoreTemplateLiterals": true
        }],
        "comma-dangle": 0,
        "no_underscore_dangle": 0,
        "indent": ["error", 4],
        "react/jsx-filename-extension": 0,
        "react/jsx-indent": ["error", 4],
        "jsx-a11y/no-static-element-interactions": 0
    },
    "globals": {
        "document": true,
        "history": true,
        "window": true
    }
}
